   },
   "outputs": [],
   "source": [
    "house_df=pd.DataFrame([Sqm,Price]).T\n",
    "house_df.columns = ['Sqm', 'Price']\n",
    "house_df['Sqm2'] = house_df.Sqm**2\n",
    "house_df['Sqm3'] = house_df.Sqm**3\n",
    "house_df['Sqm4'] = house_df.Sqm**4\n",
    "house_df['Sqm5'] = house_df.Sqm**5\n",
    "house_df['Sqm6'] = house_df.Sqm**6\n",
    "\n",
    "linear = sm.OLS(house_df['Price'],house_df[['Sqm']]).fit()\n",
    "two = sm.OLS(house_df['Price'],house_df[['Sqm', 'Sqm2']]).fit()\n",
    "three = sm.OLS(house_df['Price'],house_df[['Sqm', 'Sqm2', 'Sqm3']]).fit()\n",
    "four = sm.OLS(house_df['Price'],house_df[['Sqm', 'Sqm2', 'Sqm3', 'Sqm4']]).fit()\n",
    "five = sm.OLS(house_df['Price'],house_df[['Sqm', 'Sqm2', 'Sqm3', 'Sqm4', 'Sqm5']]).fit()\n",
    "six = sm.OLS(house_df['Price'],house_df[['Sqm', 'Sqm2', 'Sqm3', 'Sqm4', 'Sqm5', 'Sqm6']]).fit()\n",
    "\n",
    "lin_beta = linear.params['Sqm']\n",
    "beta1_2, beta2_2 = two.params['Sqm'], two.params['Sqm2']\n",
    "beta1_3, beta2_3, beta3_3 = three.params['Sqm'], three.params['Sqm2'], three.params['Sqm3']\n",
    "beta1_4, beta2_4, beta3_4, beta4_4 = four.params['Sqm'], four.params['Sqm2'], four.params['Sqm3'], four.params['Sqm4']\n",
    "beta1_5, beta2_5, beta3_5, beta4_5,beta5_5  = five.params['Sqm'], five.params['Sqm2'], five.params['Sqm3'], five.params['Sqm4'],five.params['Sqm5']\n",
    "beta1_6, beta2_6, beta3_6, beta4_6,beta5_6,beta6_6  = six.params['Sqm'], six.params['Sqm2'], six.params['Sqm3'], six.params['Sqm4'],six.params['Sqm5'],six.params['Sqm6']\n",
    "\n",
    "yvals = {0:house_df.Sqm*lin_beta,\n",
    "         1:house_df.Sqm*beta1_2 + house_df.Sqm**2*beta2_2,\n",
    "         2:house_df.Sqm*beta1_3 + house_df.Sqm**2*beta2_3 + house_df.Sqm**3*beta3_3 ,\n",
    "         3:house_df.Sqm*beta1_4 + house_df.Sqm**2*beta2_4 + house_df.Sqm**3*beta3_4 + house_df.Sqm**4*beta4_4, \n",
    "         4:house_df.Sqm*beta1_5 + house_df.Sqm**2*beta2_5 + house_df.Sqm**3*beta3_5 + house_df.Sqm**4*beta4_5 + house_df.Sqm**5*beta5_5,\n",
    "         5:house_df.Sqm*beta1_6 + house_df.Sqm**2*beta2_6 + house_df.Sqm**3*beta3_6 + house_df.Sqm**4*beta4_6 + house_df.Sqm**5*beta5_6+ house_df.Sqm**6*beta6_6}\n",
    "\n",
    "title = {0:'linear',\n",
    "         1:'2nd Degree Polynomial',\n",
    "         2:'3rd Degree Polynomial',\n",
    "         3:'4th Degree Polynomial', \n",
    "         4:'5th Degree Polynomial',\n",
    "         5:'6th Degree Polynomial'}\n",
    "\n",
    "def plot_ex():\n",
    "    fig, ax = plt.subplots(2, 3, figsize = (15, 8))\n",
    "    idx= 0\n",
    "    for r in range(2):\n",
    "        for c in range(3):\n",
    "            ax[r, c].scatter(x = Sqm, y = Price, color = col[2])\n",
    "            ax[r,c].set_xticks([])\n",
    "            ax[r,c].set_yticks([])\n",
    "            ax[r,c].set_xlabel('Square Meters')\n",
    "            ax[r,c].set_ylabel('Price')\n",
    "            ax[r,c].set_title(title[idx])\n",
    "            ax[r,c].plot(house_df.Sqm, yvals[idx], color = col[0])\n",
    "            idx+=1 \n",
    "\n",
    "    plt.tight_layout()\n",
    "    plt.show()"
   ]
