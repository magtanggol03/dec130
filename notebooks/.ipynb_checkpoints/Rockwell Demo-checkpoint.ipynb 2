    "RFR = RandomForestRegressor(max_depth = 3) #Create and Fit a Random Forest Regressor Model\n",
    "RFR.fit(house_df.drop(['Price'], axis = 1), house_df['Price'])\n",
    "rfr_vals = RFR.predict(house_df.drop(['Price'], axis = 1)) #Get Predictions from Model\n",
    "\n",
    "plt.figure(figsize = (10,8)) #Plotting\n",
    "plt.scatter(x = Sqm, y = Price, color = col[2])\n",
    "plt.xlabel('Square Meters')\n",
    "plt.ylabel('Price')\n",
    "plt.title('House Prices vs Size')\n",
    "pylab.plot(house_df.Sqm, rfr_vals , color = col[0])\n",
