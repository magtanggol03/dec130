   "source": [
    "feature1 = 'sepal length (cm)'\n",
    "feature2 = 'petal width (cm)'\n",
    "label = 'species'\n",
    "\n",
    "LR = LogisticRegression() #Fit a Logistic Regression Model\n",
    "LR.fit(iris[[feature1, feature2]], iris[label])\n",
    "\n",
    "\n",
    "plot_decision_region(iris[[feature1, feature2]], iris[label], LR)"
   ]
  },
  {
